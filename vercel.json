{
  "crons": [
    {
      "path": "/api/reminders/run",
      "schedule": "0 14 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "partytab.vercel.app"
        }
      ],
      "destination": "https://partytab.app/",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "partytab.vercel.app"
        }
      ],
      "destination": "https://partytab.app/:path*",
      "permanent": true
    }
  ]
}
